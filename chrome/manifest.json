{
  "name":"Trello to GitHub Issue",
  "version":"1.0.1",
  "description":"Create GitHub issues from Trello cards",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches":["https://trello.com/card/*", "https://trello.com/board/*"],
      "js":["lib/jquery-1.9.1.js", "lib/bootstrap-typeahead.js", "lib/trello.js", "main.js"],
      "css":["css/bootstrap.css"]
    }
  ],
  "permissions":[
    "https://api.github.com/",
    "https://api.trello.com/",
    "https://github-oauth-proxy.jit.su/"
  ],
  "web_accessible_resources": [
    "popover.html"
  ]
}
